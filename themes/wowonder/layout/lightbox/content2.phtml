function imgModal(elm){
    var img = elm.attr('data-url');
    var postID = elm.attr('data-id');
    var image_id = elm.attr('data-image-id');
   
    $("#postID").val(postID);
    $("#imagePath").val(img);
    $("#imageID").val(image_id)
    $("#imgModalImg").attr("src",img);
    $("#perImageModal").modal('show');
}

function call(){
    var postID  = $("#postID").val();
    $.ajax({
         url: Wo_Ajax_Requests_File() + '?f=get-user',
         method: 'GET',
         dataType: 'json',
         data : {
            'id' : postID,  
         },
         success:function(res){
             if(res.user_id.user_id != res.logged_in_user){ 
                document.location.href = 'https://revolus.com/messages/'+res.user_id.user_id;
             }
             else{
                 $(".call").hide();
                 $(".vcall").hide();
                 $(".follow").hide();
             }
         },
         error:function(res){
             console.log(res);
         }
    })
}


function follow(){
    var postID  = $("#postID").val();
    var text = $("#text").val();
    
    $.ajax({
         url: Wo_Ajax_Requests_File() + '?f=validate_follow',
         method: 'GET',
         dataType: 'json',
         data : {
            'id' : postID,  
         },
         success:function(res){
           if(text == "Follow"){
            var c = "-";
            $("#followUnFollow").html(c);
           }
           else if(text == "UnFollow"){
            var c = "+";
            $("#followUnFollow").html(c);
           }
           else{
                var c = "+";
               $("#followUnFollow").html(c);
           }
         },
         error:function(res){
             console.log(res);
         }
    })
}

function download(){
    var postID  = $("#postID").val();
    var imagePath = $("#imagePath").val();
    var image = $("#image").val();
    var img_to_load = imagePath;
    imgWindow = window.open(img_to_load, 'imgWindow');


